<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B3A5C1">>

    <TextView
        android:id="@+id/ServiceRequestLabel"
        android:layout_width="367dp"
        android:layout_height="41dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="48dp"
        android:background="@drawable/login_pagetitle_format"
        android:text="Revue de succursale"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/serviceLabel2"
        android:layout_width="365dp"
        android:layout_height="66dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="108dp"
        android:text="Choisissez votre niveau de satisfaction sur 5:"
        android:textColor="#000000"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn5"
        android:layout_width="49dp"
        android:layout_height="49dp"
        android:layout_marginStart="328dp"
        android:layout_marginTop="188dp"
        android:text="5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn2"
        android:layout_width="49dp"
        android:layout_height="49dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="188dp"
        android:text="2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn4"
        android:layout_width="49dp"
        android:layout_height="49dp"
        android:layout_marginStart="256dp"
        android:layout_marginTop="188dp"
        android:text="4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn1"
        android:layout_width="49dp"
        android:layout_height="49dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="188dp"
        android:text="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn3"
        android:layout_width="49dp"
        android:layout_height="49dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="188dp"
        android:text="3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>